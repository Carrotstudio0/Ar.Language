استورد مكتبة_عشوائية كال عشوائي
خيارات = ["حجرة", "ورقة", "مقص"]
س = عشوائي.اختيار_عشوائي(خيارات)
اطبع("اختر: حجرة، ورقة، أو مقص:")
ع = مدخل()
اطبع("اختيار الكمبيوتر: " + س )
اطبع("اختيار الاعب: " + ع )
اذا(ع == س ):
  اطبع("تعادل")
أو(ع == "حجرة" و س == "مقص"):
   اطبع("مبروك انت كسبت ")
أو(ع == "مقص" و س == "حجرة"):
   اطبع("مبروك خسرت بقا")
أو(ع == "حجرة" و س == "ورقة"):
   اطبع("خسرت يا باشا ") 
أو(ع == "ورقة" و س == "حجرة"):
   اطبع("مبروك كسبت ")